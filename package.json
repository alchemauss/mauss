{
  "name": "mauss",
  "author": "Ignatius Bagus",
  "description": "Declarative type-safe zero-dependency SDK",
  "version": "0.2.3",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run test:unit && npm run test:perf",
    "test:unit": "uvu -r tsm src \"(spec\\.ts)\"",
    "test:perf": "uvu -r tsm test",
    "lint": "run-p lint:**",
    "lint:code": "tsc --noEmit",
    "lint:style": "prettier -c src",
    "check:clean": "npm run publish:clean -- -n",
    "publish:clean": "git clean -dfx -e node_modules",
    "publish:build": "tsc --project src",
    "publish:format": "prettier -wu **/*.{d.ts,js}",
    "prepublishOnly": "run-s publish:**",
    "postpublish": "npm run publish:clean"
  },
  "exports": {
    ".": "./index.js",
    "./api": "./api/index.js",
    "./bits": "./bits/index.js",
    "./bits/find": "./bits/find.js",
    "./guards": "./guards/index.js",
    "./math": "./math/index.js",
    "./math/set": "./math/set.js",
    "./utils": "./utils/index.js",
    "./utils/toolbox": "./utils/toolbox.js",
    "./web": "./web/index.js",
    "./package.json": "./package.json",
    "./prettier.json": "./prettier.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "prettier": "./prettier.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/alchemauss/mauss.git"
  },
  "bugs": {
    "url": "https://github.com/alchemauss/mauss/issues"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.10",
    "esbuild": "^0.14.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "tsm": "^2.2.1",
    "typescript": "^4.1.2",
    "uvu": "^0.5.3"
  }
}
