  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devmauss/mauss.git"
  },
  "bugs": {
    "url": "https://github.com/devmauss/mauss/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
