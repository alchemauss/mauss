{
  "name": "mauss",
  "author": "Ignatius Bagus",
  "description": "Zero-dependency development kit",
  "version": "0.0.7",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "tsc -w --noEmit",
    "publish:clean": "git clean -dfx -e node_modules",
    "publish:build": "tsc -d --outDir .",
    "publish:format": "prettier -wu **/*.{d.ts,js}",
    "prepublishOnly": "run-s publish:**"
  },
  "exports": {
    ".": "./index.js",
    "./api": "./api/index.js",
    "./bits": "./bits/index.js",
    "./guards": "./guards/index.js",
    "./utils": "./utils/index.js",
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devmauss/mauss.git"
  },
  "bugs": {
    "url": "https://github.com/devmauss/mauss/issues"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
