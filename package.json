{
  "name": "mauss",
  "author": "Ignatius Bagus",
  "description": "A declarative approach to a fast and efficient type-safe SDK",
  "version": "0.3.3",
  "license": "MIT",
  "type": "module",
  "types": "./core/index.d.ts",
  "scripts": {
    "clean": "git add * && git clean -dfx -e node_modules",
    "test": "npm run test:unit",
    "test:unit": "uvu -r tsm src \"(spec\\.ts)\"",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "tsc --noEmit",
    "lint:style": "prettier -c \"src/**/*.ts\"",
    "pub:build": "tsc --project src",
    "pub:format": "prettier -wu **/*.{d.ts,js}",
    "prepublishOnly": "npm run pub:build && npm run pub:format"
  },
  "exports": {
    ".": "./core/index.js",
    "./api": "./api/index.js",
    "./bits": "./bits/index.js",
    "./bits/find": "./bits/find.js",
    "./guards": "./guards/index.js",
    "./math": "./math/index.js",
    "./math/set": "./math/set.js",
    "./utils": "./utils/index.js",
    "./utils/toolbox": "./utils/toolbox.js",
    "./web": "./web/index.js",
    "./typings": "./typings/index.d.ts",
    "./package.json": "./package.json",
    "./prettier.json": "./prettier.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "prettier": "./prettier.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/alchemauss/mauss.git"
  },
  "bugs": {
    "url": "https://github.com/alchemauss/mauss/issues"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@types/node-fetch": "^2.5.10",
    "prettier": "^2.3.0",
    "tsm": "^2.2.1",
    "typescript": "^4.1.2",
    "uvu": "^0.5.3"
  }
}
