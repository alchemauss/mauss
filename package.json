{
  "name": "mauss",
  "author": "Ignatius Bagus",
  "description": "Zero-dependency development kit",
  "version": "0.0.3",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "tsc -w --noEmit",
    "build": "tsc -d && prettier -w src",
    "clean": "git clean -dfx -e node_modules",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json",
    ".": "./src/index.js",
    "./api": "./src/api/index.js",
    "./bits": "./src/bits/index.js",
    "./guards": "./src/guards/index.js",
    "./utils": "./src/utils/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devmauss/mauss.git"
  },
  "bugs": {
    "url": "https://github.com/devmauss/mauss/issues"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "prettier": "^2.2.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
